{
  "name": "react-ts-boilerplate",
  "version": "1.0.0",
  "description": "A simple ts react boilerplate",
  "main": "index.js",
  "author": "RyanCCollins",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "start": "node devServer.js",
    "predeploy": "rimraf public/*.*",
    "deploy": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.prod.js",
    "setup:yarn": "yarn",
    "serve:bundle": "cross-env NODE_ENV=production PORT=1337 node server.js",
    "postinstall": "npm run deploy"
  },
  "dependencies": {
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.35",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.35",
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "github-markdown-css": "^2.4.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "3.0.0-beta.2",
    "react-markdown": "^2.4.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "styled-components": "^1.2.1",
    "ts-loader": "^1.3.3",
    "tslint": "^4.2.0",
    "tslint-loader": "^3.3.0",
    "tslint-react": "^2.2.0",
    "typescript": "next",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.10",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
