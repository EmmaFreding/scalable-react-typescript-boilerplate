{
  "name": "react-ts-boilerplate",
  "version": "1.0.0",
  "description": "A simple ts react boilerplate",
  "main": "index.js",
  "author": "RyanCCollins",
  "license": "MIT",
  "engines": {
    "node": "6.2.0",
    "npm": "3.8.9"
  },
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "start": "node devServer.js",
    "move:assets": "cross-env cp ./README.md build/src/containers/Docs",
    "predeploy": "rimraf build/public/*.*",
    "deploy": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.prod.js",
    "postdeploy": "./node_modules/.bin/tsc && npm run move:assets",
    "setup:yarn": "yarn",
    "serve:bundle": "cross-env NODE_ENV=server PORT=1337 node server.js",
    "postinstall": "npm run deploy"
  },
  "dependencies": {
    "@types/node": "^6.0.54",
    "@types/react": "^0.14.55",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.35",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.35",
    "@types/whatwg-fetch": "^0.0.33",
    "autoprefixer": "^6.6.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "github-markdown-css": "^2.4.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "morgan": "^1.7.0",
    "postcss-loader": "^1.2.1",
    "precss": "^1.4.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "3.0.0-beta.2",
    "react-markdown": "^2.4.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-async-connect": "^1.0.0-rc4",
    "redux-connect": "^5.0.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.1",
    "styled-components": "^1.2.1",
    "ts-loader": "^1.3.3",
    "tslint": "next",
    "tslint-loader": "^3.3.0",
    "tslint-react": "^2.2.0",
    "typescript": "next",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.10",
    "webpack-manifest-plugin": "^1.1.0",
    "whatwg-fetch": "^2.0.1"
  }
}
